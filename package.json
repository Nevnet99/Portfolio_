    "lodash": "^4.17.15",
    "matter-js": "^0.14.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "react-slick": "^0.25.2",
    "react-spring": "^8.0.27",
    "sharp": "^0.25.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "chalk": "^3.0.0",
    "contentful-import": "^7.7.0",
    "dotenv": "^8.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
